{
  "name": "react-setup",
  "version": "1.0.0",
  "description": "React setup from scartch",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"yarn run sw:start\" \"webpack-dev-server --config webpack.dev.js --mode development --open\"",
    "test": "jest",
    "test-commit": "jest --coverage",
    "test-update": "jest --updateSnapshot",
    "build": "concurrently \"yarn run sw:build\" \"webpack --config webpack.prod.js --mode production\"",
    "doc:dev": "mkdocs serve -f documentation/mkdocs.yml",
    "generate-sw": "workbox inject:manifest",
    "sw:start": "webpack --config public/webpack.config.js --mode development --watch",
    "sw:build": "webpack --config public/webpack.config.js --mode production"
  },
  "author": "Subhash Chandra",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
